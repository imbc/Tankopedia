{% block menu %}
    {% spaceless %}
    <div>HARD CODED VALUE</div>
        {% for id, category in root.children %}
        <a href="{% if category.route == null %}#{% else %}{{ category.route }}{% endif %}" id="{{ id }}">{{ category.label }}</a>
            {% if category.children | length > 0 %}
            <ul>
            {% for k, item in category.children %}
                <li>
                    <a href="{{ path( item.route ) }}">
                        {{ item.label }}
                    </a>
                </li>
            {% endfor %}
            </ul>
            {% endif %}
        {% endfor %}
    <div>HARD CODED VALUE</div>
    {% endspaceless %}
{% endblock menu %}




